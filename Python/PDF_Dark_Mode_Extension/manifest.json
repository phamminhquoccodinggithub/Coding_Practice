{
  "manifest_version": 3,
  "name": "PDF Dark Mode",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_icon": {
      "16": "rainbow.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*.pdf",
        "file:///*/*.pdf"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}